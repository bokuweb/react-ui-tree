{
  "compilerOptions": {
    "target": "ES5",
    // "module": "ES2015",
    "lib": ["es5", "es2015", "dom"],
    "allowJs": true,
    "jsx": "react",
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  },
  "exclude": ["stories", "lib"],
  "include": ["src"]
}
